        <stringProp name="parameters"></stringProp>
        <stringProp name="script">import org.apache.jmeter.services.FileServer;

String ngsiSetting = vars.get(&quot;NGSI10_SPECS_PATH&quot;);
String path, ngsi9, ngsi10;

if (ngsiSetting==null || ngsiSetting.length()==0) {
	
	path = vars.get(&quot;ABSOLUTE_PATH&quot;);
	// path = FileServer.getFileServer().getBaseDir() + File.separator;
	ngsi9 = vars.get(&quot;NGSI9_SPECS_REL_PATH&quot;);
	ngsi10 = vars.get(&quot;NGSI10_SPECS_REL_PATH&quot;);
	
	//vars.put(&quot;ABSOLUTE_PATH&quot;, path);
	vars.put(&quot;NGSI9_SPECS_PATH&quot;, path + ngsi9);
	vars.put(&quot;NGSI10_SPECS_PATH&quot;, path + ngsi10);
	
	log.info(&quot;ABSOLUTE_PATH=&quot; + vars.get(&quot;ABSOLUTE_PATH&quot;));
	log.info(&quot;NGSI9_SPECS_PATH=&quot; + vars.get(&quot;NGSI9_SPECS_PATH&quot;));
	log.info(&quot;NGSI10_SPECS_PATH=&quot; + vars.get(&quot;NGSI10_SPECS_PATH&quot;));

}</stringProp>
