	msg = msg.replaceAll(&quot;&amp;amp;&quot;,&quot;&amp;&quot;);
	return msg.replaceAll(&quot;\\&lt;[^&gt;]*&gt;&quot;,&quot;&quot;);
}

private String removeComma(String msg){
	return msg.replaceAll(&quot;,&quot;,&quot;&quot;);
}

