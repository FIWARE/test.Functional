          <JSR223PostProcessor guiclass="TestBeanGUI" testclass="JSR223PostProcessor" testname="getCredentials" enabled="true">
            <stringProp name="cacheKey">true</stringProp>
            <stringProp name="filename"></stringProp>
            <stringProp name="parameters"></stringProp>
            <stringProp name="script">import java.util.Map;
import java.util.TreeMap;
import java.util.Arrays;
import java.util.List;
import java.io.IOException;
import org.apache.jmeter.services.FileServer;

String[] attributes = [&quot;clientId&quot;, &quot;clientSecret&quot;, &quot;pep_proxyId&quot;, &quot;pep_proxyPassword&quot;,];
List&lt;String&gt; list = Arrays.asList(attributes);

TreeMap&lt;String, String&gt; sorted = new TreeMap&lt;String, String&gt;(); 
for (Map.Entry entry : vars.entrySet()) {
	if (list.contains(entry.getKey()))
		sorted.put(entry.getKey(), entry.getValue());	
}

String credentials = &quot;Credentials \n&quot;;
for (Map.Entry&lt;String, Integer&gt; entry : sorted.entrySet()) 
	credentials += entry.getKey().toString() + &quot; = &quot; +  entry.getValue().toString() + &quot;\n&quot;; 

log.info(&quot;TXT \n&quot; + credentials);
//path txt file
String file = FileServer.getFileServer().getBaseDir() + &quot;/credentials.txt&quot;;

try {
	FileWriter fw = new FileWriter(file,true);
     fw.write(credentials);
     fw.close();
} catch(IOException ioe) {
     System.err.println(&quot;IOException: &quot; + ioe.getMessage());
}</stringProp>
